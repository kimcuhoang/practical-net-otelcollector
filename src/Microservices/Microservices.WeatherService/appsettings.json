{
  "ObservabilityOptions": {
    "ServiceName": "Weather",
    "EnabledTracing": true,
    "EnabledMetrics": true,
    "CollectorUrl": "http://localhost:4317",
    "Serilog": {
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "System": "Warning",
          "Microsoft.Hosting.Lifetime": "Information",
          "Microsoft.EntityFrameworkCore": "Error",
          "Microsoft.EntityFrameworkCore.Database.Command": "Information",
          "Grpc": "Error"
        }
      }
    }
  }
}